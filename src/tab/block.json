{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "brand/tab",
	"version": "0.1.0",
	"title": "Tab",
	"category": "design",
	"icon": "layout",
	"description": "Example static block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"parent": ["mdc/tabs" ],
	"textdomain": "mdc-tabs",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"verticalAlignment": {
			"type": "string"
		},
		"width": {
			"type": "string"
		},
		"allowedBlocks": {
			"type": "array"
		},
		"templateLock": {
			"type": [ "string", "boolean" ],
			"enum": [ "all", "insert", false ]
		},
		"style": {
			"type": "string"
		},
		"tabId":{
			"type": "string"
		},
		"tabScreenIndex":{
			"type": "number"
		}

	}
}
